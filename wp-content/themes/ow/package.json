{
    "name": "crunch",
    "version": "6.3.0",
    "private": true,
    "description": "Crunch is a WordPress starter theme based on Bootstrap 4",
    "scripts": {
        "start": "npm run build -- --watch && webpack-dev-server --progress --colors",
        "build": "webpack --progress --hide-modules",
        "production": "npm run build --production",
        "prod": "npm run production",
        "cypress:open": "cypress open",
        "fav": "webpack --config-name favicon --progress --hide-modules --production",
        "lint-staged": "lint-staged",
        "stylelint-staged": "stylelint --syntax scss"
    },
    "keywords": [
        "Crunch"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "echo \"\\x1b[101m\\x1b[97m  üê∂üê∂ [ PAW Patrol in action! ]  \\x1b[0m\" && lint-staged && echo \"\\x1b[101m\\x1b[97m  üê∂üê∂ [ PAW Patrol made it! ]  \\x1b[0m\""
        }
    },
